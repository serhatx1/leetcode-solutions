let hashT={}
    for(let i =0;i<nums.length;i++){
        if(hashT[nums[i]]!==undefined &&nums[i]*2==target){
            return [hashT[nums[i]],i]
        }
        else if(hashT[target-nums[i]]!==undefined){
            return [hashT[target-nums[i]],i]
        }
        else{
            hashT[nums[i]]=i
        }
    }
    return []
